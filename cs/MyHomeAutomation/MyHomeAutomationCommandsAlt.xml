    </PhraseList>

    <PhraseList Label="group">

    </PhraseList>

    <PhraseList Label="page">
      <Item>Dashboard</Item>
      <Item>Temperature</Item>
      <Item>Sensors</Item>
      <Item>Switches</Item>
      <Item>Scenes</Item>
      <Item>Groups</Item>
      <Item>Rooms</Item>
      <Item>Utility</Item>
      <Item>Weather</Item>
    </PhraseList>

