<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="MyHomeAutomationCommandSet_en-us">
    <AppName> Its me </AppName>
    <Example> Turn on livingroom lights </Example>

    <Command Name="turnOnSwitch">
      <Example> Turn on livingroom lights. </Example>
      <ListenFor> turn on [my] {switch}</ListenFor>
      <ListenFor> switch on [my] {switch}</ListenFor>
      <Feedback> Turning on {switch} </Feedback>
      <Navigate />
    </Command>

    <Command Name="turnOffSwitch">
      <Example> Turn off livingroom lights. </Example>
      <ListenFor> turn off [my] {switch}</ListenFor>
      <ListenFor> switch off [my] {switch}</ListenFor>
      <Feedback> Turning off {switch} </Feedback>
      <Navigate />
    </Command>

    <Command Name="ToggleSwitch">
      <Example> Toggle livingroom lights. </Example>
      <ListenFor> toggle [my] {switch}</ListenFor>
      <Feedback> Toggling {switch} </Feedback>
      <Navigate />
    </Command>

    <Command Name="SetDimmer">
      <Example> Set Porch light to 60 percent. </Example>
      <ListenFor> set [my] {dimmer} [to] {percent} [percent]</ListenFor>
      <ListenFor> set [my] {dimmer} dimmer [switch] [to] {percent} [percent]</ListenFor>
      <Feedback> Setting {dimmer} to {percent} % </Feedback>
      <Navigate />
    </Command>

    <Command Name="sleepingSwitchOn">
      <Example> Have a Good Night! </Example>
      <ListenFor> [have] [a] good night </ListenFor>
      <ListenFor> sleeptight </ListenFor>
      <ListenFor> sweet dreams </ListenFor>
      <ListenFor> I'm off to bed </ListenFor>
      <Feedback> Goodnight </Feedback>
      <Navigate />
    </Command>

    <Command Name="sleepingSwitchOff">
      <Example> Good Morning. </Example>
      <ListenFor> [have] [a] good morning </ListenFor>
      <ListenFor> Where is my coffee </ListenFor>
      <Feedback> Goodmorning to you too! </Feedback>
      <Navigate />
    </Command>

    <Command Name="homeSwitchOn">
      <Example> I'm home. </Example>
      <ListenFor> I'm home </ListenFor>
      <ListenFor> I'm back </ListenFor>
      <Feedback> Welcome home. </Feedback>
      <Navigate />
    </Command>

    <Command Name="homeSwitchOff">
      <Example> I'm heading out. </Example>
      <ListenFor> I'm heading out </ListenFor>
      <Feedback> See you later! </Feedback>
      <Navigate />
    </Command>

    <Command Name="switchOnRoomLights">
      <Example> Switch on all livingroom lights. </Example>
      <ListenFor> Switch on all {room} lights </ListenFor>
      <ListenFor> Turn on all {room} lights </ListenFor>
      <Feedback> Switching on all {room} lights. </Feedback>
      <Navigate />
    </Command>

    <Command Name="switchOffRoomLights">
      <Example> Switch off all livingroom lights. </Example>
      <ListenFor> Switch off all {room} lights </ListenFor>
      <ListenFor> Turn off all {room} lights </ListenFor>
      <Feedback> Switching off all {room} lights. </Feedback>
      <Navigate />
    </Command>

    <Command Name="switchOnScene">
      <Example> Switch on movie scene. </Example>
      <ListenFor> Scene {scene} on </ListenFor>
      <ListenFor> Switch on [the] {scene} scene </ListenFor>
      <ListenFor> Switch to [the] {scene} scene </ListenFor>
      <ListenFor> Switch on [the] scene {scene} </ListenFor>
      <ListenFor> Switch to [the] scene {scene} </ListenFor>
      <ListenFor> Turn on {scene} scene </ListenFor>
      <Feedback> Switching on {scene} scene. </Feedback>
      <Navigate />
    </Command>

    <Command Name="switchOnGroup">
      <Example> Switch on garden group. </Example>
      <ListenFor> Group {group} on </ListenFor>
      <ListenFor> Switch on [the] {group} group </ListenFor>
      <ListenFor> Turn on [the] {group} group </ListenFor>
      <ListenFor> Switch on [the] group {group} </ListenFor>
      <ListenFor> Turn on [the] group {group} </ListenFor>
      <Feedback> Switching on {group} group. </Feedback>
      <Navigate />
    </Command>

    <Command Name="switchOffGroup">
      <Example> Switch off garden group. </Example>
      <ListenFor> Group {group} off </ListenFor>
      <ListenFor> Switch off [the] {group} group </ListenFor>
      <ListenFor> Turn off [the] {group} group </ListenFor>
      <ListenFor> Switch off [the] group {group} </ListenFor>
      <ListenFor> Turn off [the] group {group} </ListenFor>
      <Feedback> Switching off {group} group. </Feedback>
      <Navigate />
    </Command>
    
    <Command Name="showPage">
      <Example> Show Dashboard. </Example>
      <ListenFor> Show [the] {page} [page]</ListenFor>
      <ListenFor> Navigate to [the] {page} [page]</ListenFor>
      <ListenFor> Goto [the] {page} [page]</ListenFor>
      <ListenFor> Browse to [the] {page} [page]</ListenFor>
      <Feedback> Show {page}. </Feedback>
      <Navigate />
    </Command>

    <PhraseList Label="switch">

    </PhraseList>

    <PhraseList Label="dimmer">

    </PhraseList>

    <PhraseList Label="room">

    </PhraseList>

